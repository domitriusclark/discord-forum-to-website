{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@lib/*": ["src/lib/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
